- if @basic_eligibility.answers.try(:any?)
  .previous-answers
    .done-questions
      article
        table.eligibility-previous-answers
          thead
            tr
              th.previous-answers-title colspan="2"
                ' Your eligibility answers
              /th.link-right.restart
                = link_to "Start again", form_award_eligibility_path(form_id: @form_answer.id)
          tbody
            - @already_has_invalid_questions = false
            = render "answers", eligibility: @basic_eligibility if @basic_eligibility
